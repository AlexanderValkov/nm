[
  {
    "name" : "nm",
    "image": "nginx",
    "portMappings": [
      {
        "hostPort": 80,
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
    "cpu": 256,
    "memory": 512,
    "essential": true,
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "nm-logs",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "nm"
        }
    }
  }
]
